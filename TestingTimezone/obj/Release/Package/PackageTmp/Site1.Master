    <script>
        var d, h, m, s, animate;

        function init() {
            d = new Date();
            h = d.getHours();
            m = d.getMinutes();
            s = d.getSeconds();
            clock();
        };

        function clock() {
            s++;
            if (s == 60) {
                s = 0;
                m++;
                if (m == 60) {
                    m = 0;
                    h++;
                    if (h == 24) {
                        h = 0;
                    }
                }
            }
            ff('sec', s);
            ff('min', m);
            ff('hr', h);
            animate = setTimeout(clock, 1000);
        };

        function ff(id, val) {
            if (val < 10) {
                val = '0' + val;
            }
            document.getElementById(id).innerHTML = val;
        };

    </script>
