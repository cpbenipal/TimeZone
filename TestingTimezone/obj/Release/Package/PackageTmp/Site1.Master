    <script type="text/javascript" defer="defer">
        function getlocalTimeZone() {
            var curDate = new Date();
            var tz = /\((.*)\)/.exec(curDate.toString());
            document.getElementById("ltLocalTime").innerHTML = curDate.toLocaleDateString("en-US") + " " + tz[1];
        }
    </script>
